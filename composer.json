{
    "name": "ascend/artisan-list-mine",
    "description": "Filter artisan commands to show only your application commands",
    "keywords": ["laravel", "artisan", "console", "commands"],
    "homepage": "https://github.com/CWAscend/artisan-list-mine",
    "license": "MIT",
    "authors": [
        {
            "name": "Ascend Digital",
            "email": "dev@ascend.agency"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ascend\\ArtisanListMine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ascend\\ArtisanListMine\\Tests\\": "tests/",
            "App\\Console\\Commands\\": "tests/Fixtures/",
            "App\\Actions\\": "tests/Fixtures/",
            "Vendor\\SomePackage\\Commands\\": "tests/Fixtures/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ascend\\ArtisanListMine\\ArtisanListMineServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
